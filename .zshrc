# functions
source ~/.functions

# aliases
source ~/.aliases

# exports
source ~/.exports

# Set name of the theme to load.
ZSH_THEME="bira"

# Which plugins would you like to load? (plugins can be found in ~/.oh-my-zsh/plugins/*)
plugins=(git ruby docker brew bundler emacs gem osx rails rvm)

source $ZSH/oh-my-zsh.sh
